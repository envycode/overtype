{
  "name": "overtype",
  "description": "Learning multilanguage with gamification",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev --ext \".svelte\"",
    "build": "sapper build --legacy",
    "export": "sapper export --legacy --ext \".svelte\"",
    "start": "node __sapper__/build",
    "predev": "npm run build:tw",
    "preexport": "NODE_ENV=production npm run build:tw",
    "build:tw": "postcss src/assets/css/global.css -o static/global.css",
    "lint": "eslint --fix \"**/*.{js,svelte}\"",
    "format": "prettier --write \"./**/*.{js,yaml,svelte}\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "compression": "^1.7.1",
    "js-cookie": "^2.2.1",
    "polka": "next",
    "sirv": "^1.0.0",
    "ws": "^7.4.1"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@fullhuman/postcss-purgecss": "^3.0.0",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^14.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "@rollup/plugin-url": "^5.0.0",
    "@rollup/plugin-yaml": "^3.0.0",
    "@tailwindcss/typography": "^0.3.1",
    "autoprefixer": "^10.1.0",
    "babel-eslint": "^10.1.0",
    "cssnano": "^4.1.10",
    "dotenv": "^8.2.0",
    "eslint": "^7.15.0",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.2.0",
    "eslint-plugin-svelte3": "^2.7.3",
    "node-sass": "^5.0.0",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.0",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^1.4.2",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^6.0.0",
    "rollup-plugin-svelte-svg": "^0.2.3",
    "rollup-plugin-terser": "^7.0.0",
    "sapper": "^0.28.0",
    "svelte": "^3.17.3",
    "svelte-image": "^0.2.7",
    "svelte-preprocess": "^4.6.1",
    "tailwindcss": "^2.0.2"
  }
}
